#%Module -*- tcl -*-
#

proc ModulesHelp { } {

    puts stderr "This module adds the Keras package to your environment."

}

module-whatis "Adds Keras to your environment."


# Add binary to PATH
prereq python/3.6.3
prereq tensorflow/1.12.0

conflict keras

set root 			/shared/ucl/apps/keras/2.2.4
prepend-path PYTHONPATH         $root
