# New Legion modules layout

lappend auto_path /shared/ucl/apps/modulelibs/UsefulModuleFunctions
package require modulefunctions 1.0


proc ModulesHelp { } {
    puts stderr "Adds Rosetta 2015.31.58019 to your environment."
    puts stderr ""
    puts stderr "The Rosetta software suite includes algorithms for"
    puts stderr "computational modeling and analysis of protein structures."
    puts stderr ""
    puts stderr "Directory: /shared/ucl/apps/Rosetta/rosetta_src_2015.31.58019_bundle"
}



module-whatis "Adds  Rosetta 2015.31.58019 to your environment."

conflict rosetta

prereq gcc-libs/4.9.2
prereq compilers/gnu/4.9.2
prereq python2/recommended


# Location of Rosetta:

set prefix /shared/ucl/apps/Rosetta/rosetta_src_2015.31.58019_bundle

prepend-path 	PATH		$prefix/main/source/bin
setenv		ROSETTA3_DB	$prefix/main/database
