## Version 1.0 by BAA on Legion on 14-09-2016

proc ModulesHelp { } {
    puts stderr "This module adds the cancerit package to your environment."
    puts stderr ""
    puts stderr "This module also adds other modules as required."
    puts stderr ""
    puts stderr ""
}

module-whatis "adds UCL recommended set of cancerit packages to your environment variables"

odule load r/recommended
module load htslib/1.3.1 
module load perl/5.22.0
module load curl/7.47.1/gnu-4.9.2
module load fasta/36.3.8d/gnu-4.9.2
module load bedtools/2.25.0
module load exonerate/2.2.0
module load velvet/1.2.10
module load python2/recommended
module load boost/1_54_0/gnu-4.9.2
module load cancerit/gnu-4.9.2
