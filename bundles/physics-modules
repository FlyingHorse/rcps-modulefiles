proc ModulesHelp { } {

    puts stderr "This module adds Physics Department supported modules to your environment."
    puts stderr ""
    puts stderr "These modules are supported by local Physics IT staff."
    puts stderr ""
}

module-whatis "Adds Pysics Department supported modules to your environment."

module           use --append /shared/ucl/depts/physics/modulefiles

