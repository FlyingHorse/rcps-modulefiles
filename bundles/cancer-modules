proc ModulesHelp { } {

    puts stderr "This module adds Cancer Biologyu supported modules to your environment."
    puts stderr ""
    puts stderr "These modules are supported by local Cancer Biology IT staff."
    puts stderr ""
}

module-whatis "Adds Cancer Biology supported modules to your environment."

module           use --append /shared/ucl/depts/cancer/modulefiles
