#%Module -*- tcl -*-
#
# xorg-utils X11R7.7
#
#                                                      H.K. Aug 2015

proc ModulesHelp { } {

    puts stderr "This module adds xorg-utils from X11R7.7 to your environment."
    puts stderr "Includes util-macros-1.17 and makedepend-1.0.5."
    puts stderr ""

}

module-whatis "Adds xorg-utils from X11R7.7 to your environment. Includes util-macros-1.17 and makedepend-1.0.5."

# Add binary to PATH
prereq gcc-libs/4.9.2

conflict xorg-utils

set root 			/shared/ucl/apps/xorg/utils/X11R7.7/gnu-4.9.2
prepend-path PATH    		$root/bin
prepend-path MANPATH		$root/share/man
prepend-path PKG_CONFIG_PATH	$root/share/pkgconfig
